{
	"name": "gummi-bands",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"db:app:generate": "drizzle-kit generate",
		"db:app:generate:custom": "drizzle-kit generate --custom",
		"db:app:migrate": "drizzle-kit migrate",
		"auth:generate": "npx @better-auth/cli generate --config ./src/lib/server/auth.ts --output ./src/lib/db/server/schema.auth.ts",
		"db:server:generate": "drizzle-kit generate --config=drizzle.server.config.ts",
		"db:server:migrate": "drizzle-kit migrate --config=drizzle.server.config.ts",
		"db:server:studio": "drizzle-kit studio --config=drizzle.server.config.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@iconify/json": "^2.2.421",
		"@iconify/tailwind4": "^1.2.0",
		"@libsql/client": "^0.15.15",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-vercel": "^6.2.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^24",
		"drizzle-kit": "^0.31.8",
		"drizzle-orm": "^0.45.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"unplugin-icons": "^22.5.0",
		"vite": "^7.2.6",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.15"
	},
	"dependencies": {
		"@electric-sql/pglite": "^0.3.14",
		"@fontsource/bebas-neue": "^5.2.7",
		"@fontsource/inter": "^5.2.8",
		"@neondatabase/serverless": "^1.0.2",
		"better-auth": "^1.4.18",
		"bits-ui": "^2.14.4",
		"tw-animate-css": "^1.4.0",
		"valibot": "^1.2.0"
	}
}
